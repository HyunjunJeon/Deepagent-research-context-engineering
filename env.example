# API Keys for Deep Research Agent Example
# Copy this file to .env and fill in your actual API keys

# OpenAI API Key
OPENAI_API_KEY=your_openai_api_key_here

# Tavily API Key (for web search)
TAVILY_API_KEY=your_tavily_api_key_here

# LangSmith API Key
# Get your key at: https://smith.langchain.com/settings
LANGSMITH_API_KEY=lsv2_pt_your_api_key_here
LANGSMITH_TRACING=true
LANGSMITH_PROJECT=Deep-Agent-Context-Engineering

# LangGraph CLI - which allows synchronous blocking I/O operations
BG_JOB_ISOLATED_LOOPS=true